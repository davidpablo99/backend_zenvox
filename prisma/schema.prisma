generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId

  name      String
  email     String   @unique
  password  String
  cpf       String   @unique
  birthDate DateTime
  address   String?

  created_at DateTime @default(now())
}
